https://www.codechef.com/viewsolution/49903435

#include <iostream>
#include<bits/stdc++.h>
#define int long long
using namespace std;
const int maxi=1e6 + 10;

int a[maxi],spf[maxi],check[maxi],cnt[maxi],oddprime[maxi];

void spfi(){
    int i,j;
    for(i=1;i<=maxi;i++)
    {
        spf[i]=i;
    }
    for(i=2;i*i< (maxi);i++)
    {
        if(spf[i]==i){
            for(j=i*i;j< (maxi);j = j+i)
            {
                if(spf[j]==j){
                    spf[j]=i;
                }
            }
        }
    }
}

map<int,int> get(int n){
    map<int,int>ret;
    while(n!=1){
        ret[spf[n]]++;
        n = n/spf[n];
    }
    return ret;
}

void solve(){
    int n;
    cin>>n;
    int i;
    for(i=0;i<n;i++){
        cin>>a[i];
        a[i]=oddprime[a[i]];
        cnt[a[i]]=0;
        check[a[i]]=0;
    }
    for(i=0;i<n;i++){
        cnt[a[i]]++;
    }
    int ans = (n*(n-1))/2;
    for(i=0;i<n;i++){
        if(!check[a[i]]){
            ans -= ((cnt[a[i]]*(cnt[a[i]]-1))/2);
            check[a[i]]=1;
        }
    }
    cout<<ans<<"\n";
}

signed main() {
    spfi();
    int i;
    for(i=1;i<(maxi);i++)
    {
        map<int,int> m =get(i);
        int b=1;
        for(auto u:m){
            if(u.second%2 == 1){
                b *= u.first;
            }
        }
        oddprime[i]=b;
    }
    
    int t;
    cin>>t;
    while(t--){
        solve();
    }
	// your code goes here
	return 0;
}
